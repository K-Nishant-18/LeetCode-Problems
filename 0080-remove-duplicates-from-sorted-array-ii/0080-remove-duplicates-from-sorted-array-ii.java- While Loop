class Solution {
    public int removeDuplicates(int[] nums) {
        int i = 0;
        int k = 0;

        while (i < nums.length) {
            if (k < 2 || nums[i] != nums[k - 2]) {
                nums[k] = nums[i];
                k++;
            }
            i++;
        }
        return k;
    }
}
